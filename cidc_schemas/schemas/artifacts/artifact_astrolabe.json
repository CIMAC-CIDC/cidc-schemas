{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "astrolabe_artifact",
  "title": "Astrolabe Artifact",
  "type": "object",
  "description": "A compressed archive storing analysis and results from Astrolabe analysis.",
  "allOf": [
    {
      "$ref": "artifacts/artifact_core.json"
    },
    {
      "properties": {
        "analysis_type": {
            "type": "string",
            "enum": [
              "FCS File",
              "Reports",
              "Analysis"
            ]
        }
      }
    }
  ]
}

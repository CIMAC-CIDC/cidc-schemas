{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "wes_analysis",
  "title": "WES analysis combined files",
  "type": "object",
  "description": "Combined analysis files for the whole WES assay.",
  "additionalProperties": false,
  "properties": {
    "update_date": {
      "description": "Date files were updated.",
      "type": "string",
      "format": "date"
    },
    "pair_based": {
      "type": "array",
      "items": {
        "$ref": "assays/components/ngs/wes_pair_analysis.json"
      }
    },
    "sample_based": {
      "type": "array",
      "items": {
        "$ref": "assays/components/ngs/wes_sample_analysis.json"
      }
    }
  }
}
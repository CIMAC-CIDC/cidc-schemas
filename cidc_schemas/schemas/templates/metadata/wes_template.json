{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "wes",
  "title": "Metadata file for WES",
  "type": "object",
  "category": "metadata",
  "description": "Metadata information for whole exome sequencing analysis.\n",
  "properties": {
    "worksheets": {
      "WES": {
        "preamble_rows": {
          "lead organization study id": {
            "merge_pointer": "/lead_organization_study_id",
            "type_ref": "clinical_trial.json#properties/lead_organization_study_id"
          },
          "assay creator": {
            "relative_merge_pointer": "0/assay_creator",
            "merge_pointer": "/assays/wes/0/assay_creator",
            "type_ref": "assays/components/assay_core.json#properties/assay_creator"
          },
          "enrichment kit": {
            "relative_merge_pointer": "0/enrichment_vendor_kit",
            "merge_pointer": "/assays/wes/0/enrichment_vendor_kit",
            "type_ref": "assays/wes_assay.json#properties/enrichment_vendor_kit"
          },
          "library kit vendor": {
            "relative_merge_pointer": "0/library_vendor_kit",
            "merge_pointer": "/assays/wes/0/library_vendor_kit",
            "type_ref": "assays/components/ngs_assay_core.json#properties/library_vendor_kit"
          },
          "sequencer platform": {
            "relative_merge_pointer": "0/sequencer_platform",
            "merge_pointer": "/assays/wes/0/sequencer_platform",
            "type_ref": "assays/components/ngs_assay_core.json#properties/sequencer_platform"
          },
          "paired end reads": {
            "relative_merge_pointer": "0/paired_end_reads",
            "merge_pointer": "/assays/wes/0/paired_end_reads",
            "type_ref": "assays/components/ngs_assay_core.json#properties/paired_end_reads"
          },
          "read length": {
            "relative_merge_pointer": "0/read_length",
            "merge_pointer": "/assays/wes/0/read_length",
            "type_ref": "assays/components/ngs_assay_core.json#properties/read_length"
          }
        },
        "data_columns": {
          "Samples": {
            "cimac participant id": {
              "relative_merge_pointer": "0/records/0/cimac_participant_id", 
              "merge_pointer": "/assays/wes/0/records/0/cimac_participant_id", 
              "type_ref": "participant.json#properties/cimac_participant_id"
            },
            "cimac sample id": {
              "relative_merge_pointer": "0/records/0/cimac_sample_id", 
              "merge_pointer": "/assays/wes/0/records/0/cimac_sample_id", 
              "type_ref": "sample.json#properties/cimac_sample_id"
            },
            "cimac aliquot id": {
              "relative_merge_pointer": "0/records/0/cimac_aliquot_id", 
              "merge_pointer": "/assays/wes/0/records/0/cimac_aliquot_id", 
              "type_ref": "aliquot.json#properties/cimac_aliquot_id"
            },
            "library kit lot": {
              "relative_merge_pointer": "0/records/0/library_kit_lot", 
              "merge_pointer": "/assays/wes/0/records/0/library_kit_lot", 
              "type_ref": "assays/components/ngs/ngs_assay_record.json#properties/library_kit_lot"
            },
            "library prep date": {
              "relative_merge_pointer": "0/records/0/library_prep_date", 
              "merge_pointer": "/assays/wes/0/records/0/library_prep_date", 
              "type_ref": "assays/components/ngs/ngs_assay_record.json#properties/library_prep_date"
            },
            "dna input ng": {
              "relative_merge_pointer": "0/records/0/input_ng", 
              "merge_pointer": "/assays/wes/0/records/0/input_ng", 
              "type_ref": "assays/components/ngs/ngs_assay_record.json#properties/input_ng"
            },
            "library yield ng": {
              "relative_merge_pointer": "0/records/0/library_yield_ng", 
              "merge_pointer": "/assays/wes/0/records/0/library_yield_ng", 
              "type_ref": "assays/components/ngs/ngs_assay_record.json#properties/library_yield_ng"
            },
            "average insert size": {
              "relative_merge_pointer": "0/records/0/average_insert_size", 
              "merge_pointer": "/assays/wes/0/records/0/average_insert_size", 
              "type_ref": "assays/components/ngs/ngs_assay_record.json#properties/average_insert_size"
            },
            "enrichment lot": {
              "relative_merge_pointer": "0/records/0/enrichment_vendor_lot", 
              "merge_pointer": "/assays/wes/0/records/0/enrichment_vendor_lot", 
              "type_ref": "assays/components/ngs/wes_assay_record.json#properties/enrichment_vendor_lot"
            },
            "capture date": {
              "relative_merge_pointer": "0/records/0/capture_date", 
              "merge_pointer": "/assays/wes/0/records/0/capture_date", 
              "type_ref": "assays/components/ngs/wes_assay_record.json#properties/capture_date"
            },
            "forward fastq": {
              "relative_merge_pointer": "0/files/fastq_1", 
              "merge_pointer": "/assays/wes/0/files/fastq_1", 
              "type_ref": "assays/components/fastq_pair_and_mapping.json#properties/fastq_1",
              "load_through": "assays/components/local_file.json#properties/file_path"
            },
            "reverse fastq": {
              "relative_merge_pointer": "0/files/fastq_2", 
              "merge_pointer": "/assays/wes/0/files/fastq_2", 
              "type_ref": "assays/components/fastq_pair_and_mapping.json#properties/fastq_2",
              "load_through": "assays/components/local_file.json#properties/file_path"
            },
            "read group mapping file": {
              "relative_merge_pointer": "0/files/read_group_mapping_file", 
              "merge_pointer": "/assays/wes/0/files/read_group_mapping_file", 
              "type_ref": "assays/components/fastq_pair_and_mapping.json#properties/read_group_mapping_file",
              "load_through": "assays/components/local_file.json#properties/file_path"
            }
          }
        }
      }
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "CYTOF",
  "title": "Metadata file for CYTOF",
  "description": "Metadata information for mass-cytometry (CYTOF).",
  "properties": {
    "worksheets": {
      "Data Acquisition": {
        "preamble_rows": {
          "lead organization study id": {
            "$ref": "clinical_trial.json#properties/lead_organization_study_id"
          },
          "assay creator": {
            "$ref": "assays/components/assay_core.json#properties/assay_creator"
          },
          "assay category": {
            "$ref": "assays/components/assay_core.json#properties/assay_category"
          },
          "artifact category": {
            "$ref": "artifacts/artifact_core.json#properties/artifact_category"
          },
          "panel name": {
            "$ref": "assays/components/cytof_panel.json#properties/panel_name"
          }
        },
        "data_columns": {
          "Runs": {
            "original fcs filename(s)": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/original_fcs_filenames"
            },
            "date of acquisition": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/date_of_acquisition"
            },
            "instrument": {
              "$ref": "assays/components/cytof_platform.json#properties/instrument"
            },
            "injector": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/injector"
            },
            "acquisition_buffer": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/acquisition_buffer"
            },
            "batch_id": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/batch_id"
            },
            "average_event_per_second": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/average_event_per_second"
            },
            "run_time": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/run_time"
            },
            "notes": {
              "$ref": "assays/components/cytof_acquisition.json#definitions/cytof_acquisition/properties/notes"
            }
          }
        }
      },
      "Data Preprocessing": {
        "data_columns": {
          "Samples": {
            "cimac particpant id": {
              "$ref": "participant.json#properties/cimac_participant_id"
            },
            "cimac sample id": {
              "$ref": "sample.json#properties/cimac_sample_id"
            },
            "time point": {
              "$ref": "sample.json#properties/time_point"
            },
            "processed fcs filename": {
              "$ref": "assays/components/local_file.json#properties/file_path"
            },
            "source fcs filenames": {
              "$ref": "assays/components/local_file.json#properties/file_path"
            },
            "concatenation version": {
              "$ref": "assays/components/cytof/cytof_entry.json#properties/concatenation_version"
            },
            "normalization version": {
              "$ref": "assays/components/cytof/cytof_entry.json#properties/normalization_version"
            },
            "beads removed": {
              "$ref": "assays/components/cytof/cytof_entry.json#properties/beads_removed"
            },
            "debarcoding protocol": {
              "$ref": "assays/components/cytof/cytof_entry.json#properties/debarcoding_protocol"
            },
            "debarcoding key": {
              "$ref": "assays/components/cytof/cytof_entry.json#properties/debarcoding_key"
            },
            "batch id": {
              "$ref": "assays/components/cytof/cytof_entry.json#properties/batch_id"
            }
          }
        }
      },
      "Antibody Information": {
        "data_columns": {
          "Samples": {
            "target": {
              "$ref": "assays/components/antibody.json#properties/antibody"
            },
            "clone": {
              "$ref": "assays/components/antibody.json#properties/clone"
            },
            "antibody company": {
              "$ref": "assays/components/antibody.json#properties/company"
            },
            "catalog number": {
              "$ref": "assays/components/antibody.json#properties/cat_num"
            },
            "lot number": {
              "$ref": "assays/components/antibody.json#properties/lot_num"
            },
            "isotope": {
              "$ref": "assays/components/cytof_antibody.json#properties/isotope"
            },
            "dilution": {
              "$ref": "assays/components/cytof_antibody.json#properties/dilution"
            },
            "stain_type": {
              "$ref": "assays/components/cytof_antibody.json#properties/stain_type"
            }
          }
        }
      }
    }
  }
}

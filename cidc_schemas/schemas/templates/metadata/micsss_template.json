{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "micsss",
  "title": "Metadata file for micsss",
  "description": "Metadata information for multiplexed immunohistochemical consecutive staining on single slide.",
  "properties": {
    "worksheets": {
      "micsss": {
        "preamble_rows": {
          "lead organization study id": {
            "$ref": "clinical_trial.json#properties/lead_organization_study_id"
          },
          "assay creator": {
            "$ref": "assays/components/assay_core.json#properties/assay_creator"
          },
          "slide scanner model": {
            "$ref": "assays/components/image.json#properties/slide_scanner_model"
          },
          "staining platform": {
            "$ref": "assays/components/image.json#properties/staining_platform"
          },
          "autostainer model": {
            "$ref": "assays/components/image.json#properties/autostainer_model"
          },
          "image analysis software": {
            "$ref": "assays/components/image.json#properties/image_analysis_software"
          },
          "image analysis software version": {
            "$ref": "assays/components/image.json#properties/image_analysis_software_version"
          },
          "cell segmentation model": {
            "$ref": "assays/components/image.json#properties/cell_segmentation_model"
          },
          "positive cell detection model": {
            "$ref": "assays/components/image.json#properties/positive_cell_detection"
          },
          "protocol name": {
            "$ref": "assays/components/image.json#properties/protocol_name"
          }
        },
        "data_columns": {
          "Samples": {
            "Antibody": {
              "$ref": "assays/components/antibody.json#properties/antibody"
            },
            "Clone": {
              "$ref": "assays/components/antibody.json#properties/clone"
            },
            "Antibody Company": {
              "$ref": "assays/components/antibody.json#properties/company"
            },
            "Cat#": {
              "$ref": "assays/components/antibody.json#properties/cat_num"
            },
            "Lot#": {
              "$ref": "assays/components/antibody.json#properties/lot_num"
            },
            "Staining Order": {
              "$ref": "assays/components/antibody.json#properties/staining_order"
            },
            "Primary Antibody Block": {
              "$ref": "assays/components/micsss_antibody.json#properties/primary_antibody_block"
            },
            "PostPrimary Linker Block": {
              "$ref": "assays/components/micsss_antibody.json#properties/postPrimary_linker_block"
            },
            "Secondary Antibody Block": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_antibody_block"

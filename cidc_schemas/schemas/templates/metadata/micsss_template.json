            "Species": {
              "$ref": "assays/components/micsss_antibody.json#properties/species"
            },
            "Isotype": {
              "$ref": "assays/components/micsss_antibody.json#properties/isotype"
            },
            "Pr. Antibody Dilution": {
              "$ref": "assays/components/micsss_antibody.json#properties/primary_antibody_dilution"
            },
            "Pr. Antibody Incubation Time": {
              "$ref": "assays/components/micsss_antibody.json#properties/primary_antibody_incubation_time"
            },
            "Pr. Antibody Incubation Temp": {
              "$ref": "assays/components/micsss_antibody.json#properties/primary_antibody_incubation_temp"
            },
            "S. Antibody": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_antibody"
            },
            "S. Company": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_company"
            },
            "S. Cat #": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_cat_num"
            },
            "S. Lot #": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_lot_num"
            },
            "S. Antibody Dilution": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_antibody_dilution"
            },
            "S. Antibody Incubation Time": {
              "$ref": "assays/components/micsss_antibody.json#properties/secondary_antibody_incubation_time"
            },
            "AR Reagent": {
              "$ref": "assays/components/micsss_antibody.json#properties/ar_reagent"
            },
            "AR pH": {
              "$ref": "assays/components/micsss_antibody.json#properties/ar_ph"
            },
            "AR Incubation Time": {
              "$ref": "assays/components/micsss_antibody.json#properties/ar_incubation_time"
            }
          }
        }
      },
      "Data": {
        "preamble_rows": {
          "lead organization study id": {
            "$ref": "clinical_trial.json#properties/lead_organization_study_id"
          }
        },
        "data_columns": {
          "Samples": {

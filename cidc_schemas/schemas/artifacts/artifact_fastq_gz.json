  "$id": "artifact_fastq_gz",
  "title": "Gzipped FastQ Artifact",
  "type": "object",
  "description": "Information about a Gzipped FASTQ file.",
  "allOf": [
    {
      "$ref": "artifacts/artifact_core.json"
    },
    {
      "properties": {
        "data_format": {
          "description": "Data format.",

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "micsss_antibody",
  "title": "MICSSS antibody",
  "description": "Data specific to antibody prepration on the multiplex immunohistochemical platform\n",
  "allOf": [{ "$ref": "assays/components/antibody.json" }],
  "properties": {
    "primary_antibody_block": {
      "type": "string"
    },
    "postPrimary_linker_block": {
      "type": "string"
    },
    "secondary_antibody_block": {
      "type": "string"
    },
    "species": {
      "type": "string"
    },
    "isotype": {
      "type": "string"
    },
    "primary_antibody_dilution": {
      "type": "string"
    },
    "primary_antibody_incubation_time": {
      "type": "string",
      "format": "time"
    },
    "primary_antibody_incubation_temp": {
      "type": "string"
    },
    "secondary_antibody": {
      "type": "string"
    },
    "secondary_company": {
      "type": "string"
    },
    "secondary_cat_num": {
      "type": "number"
    },
    "secondary_lot_num": {
      "type": "number"
    },
    "secondary_antibody_dilution": {
      "type": "string"
    },
    "secondary_antibody_incubation_time": {
      "type": "string",
      "format": "time"
    },
    "ar_reagent": {
      "type": "string"
    },
    "ar_ph": {
      "type": "string"
    },
    "ar_incubation_time": {
      "type": "string",
      "format": "time"
    }
  },
  "required": ["primary_antibody_block"]
}

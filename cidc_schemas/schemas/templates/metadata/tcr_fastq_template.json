{
  "title": "Metadata file for TCR seq  [FASTQ].",
  "description": "Metadata information for rna expression data submission via FASTQ files.",
  "prism_preamble_object_schema" : "assays/tcr_assay.json",
  "prism_preamble_object_pointer" : "/assays/tcr/-",
  "prism_data_object_pointer" : "/records/-",
  "properties": {
    "worksheets": {

      "TCR": {
        "preamble_rows": {
          "protocol identifier": {
            "merge_pointer": "3/protocol_identifier",
            "type_ref": "clinical_trial.json#properties/protocol_identifier"
          },
          "assay creator": {
            "merge_pointer": "0/assay_creator",
            "type_ref": "assays/components/assay_core.json#properties/assay_creator"
          },
          "sequencer platform": {
            "merge_pointer": "0/sequencer_platform",
            "type_ref": "assays/components/ngs_assay_core.json#properties/sequencer_platform"
          },
          "batch id": {
            "merge_pointer": "0/batch_id",
            "type_ref": "assays/tcr_assay.json#properties/batch_id"
          },
          "sample sheet": {
            "merge_pointer": "0/batch/sample_sheet",
            "gcs_uri_format": "{protocol identifier}/tcr/{batch id}/sample_sheet.csv",
            "is_artifact": 1,
            "type_ref": "assays/components/local_file.json#properties/file_path"
          }
        },

        "data_columns": {
          "Samples": {
            "cimac id": {
              "merge_pointer": "0/cimac_id",
              "type_ref": "sample.json#properties/cimac_id"
            },
            "forward fastq": {
              "merge_pointer": "0/files/r1",
              "gcs_uri_format": "{protocol identifier}/tcr/{batch id}/{cimac id}/r1_{num}.fastq.gz",
              "is_artifact": "multi",
              "type_ref": "assays/components/multiple_local_files.json#properties/file_path_list"
            },
            "reverse fastq": {
              "merge_pointer": "0/files/r2",
              "gcs_uri_format": "{protocol identifier}/tcr/{batch id}/{cimac id}/r2_{num}.fastq.gz",
              "is_artifact" : "multi",
              "type_ref": "assays/components/multiple_local_files.json#properties/file_path_list"
            },
            "library yield ng": {
              "merge_pointer": "0/library_yield_ng",
              "type_ref": "assays/components/ngs/tcr/tcr_entry.json#properties/library_yield_ng"
            }
          }
        }
      }
    }
  }
}
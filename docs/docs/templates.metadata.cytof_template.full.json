{
    "$id": "CYTOF",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Metadata information for mass-cytometry (CYTOF).",
    "properties": {
        "worksheets": {
            "Antibody Information": {
                "data_columns": {
                    "Samples": {
                        "antibody company": {
                            "description": "Company from which antibody sample was derived.",
                            "type": "string"
                        },
                        "catalog number": {
                            "description": "Antibody registry catalog number assigned to antibody sample.",
                            "type": "string"
                        },
                        "clone": {
                            "description": "Resulting type of antibody clone from primary antibody sample.",
                            "type": "string"
                        },
                        "dilution": {
                            "description": "Concentration ratio of dilution buffer for primary antibody.",
                            "type": "string"
                        },
                        "isotope": {
                            "description": "Antibody isotype used to help differentiate antibody signals.",
                            "type": "string"
                        },
                        "lot number": {
                            "description": "Identification number assigned to the particular quantity or lot of material from manufacturer.",
                            "type": "string"
                        },
                        "stain_type": {
                            "description": "Type of staining method used for antibody.",
                            "enum": [
                                "Surface Stain",
                                "Intracellular"
                            ],
                            "type": "string"
                        },
                        "target": {
                            "description": "Antibody type collected for this study.",
                            "type": "string"
                        }
                    }
                }
            },
            "Data Acquisition": {
                "data_columns": {
                    "Runs": {
                        "acquisition_buffer": {
                            "description": "Cell staining buffer used for antibody and cell dilution.",
                            "type": "string"
                        },
                        "average_event_per_second": {
                            "description": "Analysis rate of cyTOF expressions per second.",
                            "type": "number"
                        },
                        "batch_id": {
                            "description": "CyTOF batch identifaction number.",
                            "type": "string"
                        },
                        "date of acquisition": {
                            "description": "Date of cyTOF batch acquisition.",
                            "type": "string"
                        },
                        "injector": {
                            "description": "Name of Injector component used as part of the CyTOF software",
                            "type": "string"
                        },
                        "instrument": {
                            "description": "CyTOF instrument used for experiment.",
                            "type": "string"
                        },
                        "notes": {
                            "description": "Any notes pertaining to cyTOF acquisition.",
                            "type": "string"
                        },
                        "original fcs filename(s)": {
                            "description": "Normalized cyTOF FCS filenames.",
                            "type": "string"
                        },
                        "run_time": {
                            "description": "Length of run time for cyTOF experiment.",
                            "type": "string"
                        }
                    }
                },
                "preamble_rows": {
                    "artifact category": {
                        "description": "Artifact category.",
                        "enum": [
                            "Assay Artifact from CIMAC",
                            "Pipeline Artifact",
                            "Manifest File",
                            "Other"
                        ],
                        "type": "string"
                    },
                    "assay category": {
                        "description": "Assay category.",
                        "enum": [
                            "Whole Exome Sequencing (WES)",
                            "RNASeq",
                            "Conventional Immunohistochemistry",
                            "Multiplex Immunohistochemistry",
                            "Multiplex Immunofluorescence",
                            "CyTOF",
                            "OLink",
                            "NanoString",
                            "ELISpot",
                            "Multiplexed Ion-Beam Imaging (MIBI)",
                            "Other",
                            "None"
                        ],
                        "type": "string"
                    },
                    "assay creator": {
                        "description": "Indicates what site is filling out the assay.",
                        "enum": [
                            "DFCI",
                            "Mount Sinai",
                            "Stanford",
                            "MD Anderson"
                        ],
                        "type": "string"
                    },
                    "lead organization study id": {
                        "description": "Trial identifier used by lead organization, e.g. Center for Experimental Therapeutics Program (CTEP) ID or Industry Sponsored ID.  This is usually a short identifier. Example: E4412.",
                        "type": "string"
                    },
                    "panel name": {
                        "description": "Standardized CyTOF panel name used for antibody sample development.",
                        "type": "string"
                    }
                }
            },
            "Data Preprocessing": {
                "data_columns": {
                    "Samples": {
                        "batch id": {
                            "description": "Identification number for CyTOF assay batch.",
                            "type": "string"
                        },
                        "beads removed": {
                            "description": "Confirmation of beads being removed after normalization process.",
                            "enum": [
                                "Y",
                                "N"
                            ],
                            "type": "string"
                        },
                        "cimac particpant id": {
                            "description": "Participant identifier assigned by the CIMAC-CIDC Network. Example: CIMAC-1234.",
                            "type": "string"
                        },
                        "cimac sample id": {
                            "description": "Specimen identifier assigned by the CIMAC-CIDC Network. Example: CIMAC-12453.",
                            "type": "string"
                        },
                        "concatenation version": {
                            "description": "Concatenation of FCS files into a single FCS version number.",
                            "type": "string"
                        },
                        "debarcoding key": {
                            "description": "Key required for access to the Debarcoding software.",
                            "type": "string"
                        },
                        "debarcoding protocol": {
                            "description": "Protocol for the download and installation of the Debarcoder CyTOF software feature.",
                            "type": "string"
                        },
                        "normalization version": {
                            "description": "Version of normalization for CyTOF assay batch.",
                            "type": "string"
                        },
                        "processed fcs filename": {
                            "description": "Path to a file on a users computer",
                            "type": "string"
                        },
                        "source fcs filenames": {
                            "description": "Path to a file on a users computer",
                            "type": "string"
                        },
                        "time point": {
                            "description": "Study related collection time point, as described in the protocol intake form. Example: Progression.",
                            "title": "Time Point",
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "title": "Metadata file for CYTOF"
}
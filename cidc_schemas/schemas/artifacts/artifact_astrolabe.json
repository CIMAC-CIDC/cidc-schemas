{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "astrolabe_artifact",
  "title": "Astrolabe Artifact",
  "type": "object",
  "description": "A compressed archive storing results from Astrolabe analysis",
  "allOf": [
    {
      "$ref": "artifacts/artifact_core.json"
    },
    {
      "properties": {
        "analysis_type": {
            "type": "string",
            "enum": ["Gated FCS", "Reports", "R Files"]
        }
      }
    }
  ]
}
